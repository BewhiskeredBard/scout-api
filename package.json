{
  "name": "scout-api",
  "version": "0.0.0",
  "description": "An unofficial JavaScript library for the Scout security system API.",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf generated-src/** lib/**",
    "generate": "openapi-generator generate -i openapi.yml -g typescript-axios -o generated-src/",
    "test": "jest",
    "compile": "tsc",
    "build": "npm test && npm run lint && npm run compile",
    "release": "npm run clean && npm run generate && npm run build",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanryanmoore/scout-api-js.git"
  },
  "author": "Jordan Ryan Moore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordanryanmoore/scout-api-js/issues"
  },
  "homepage": "https://github.com/jordanryanmoore/scout-api-js#readme",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.8-4.2.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.25",
    "@types/jsonwebtoken": "^8.3.5",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "acorn": "^7.1.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "jsonwebtoken": "^8.5.1"
  }
}
