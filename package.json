{
  "name": "scout-api",
  "version": "0.3.0",
  "description": "An unofficial JavaScript library for the Scout security system API.",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "pregenerate": "rm -rf generated-src/**",
    "generate": "openapi-generator generate -i openapi.yml -g typescript-axios -o generated-src/ -c openapi.config.yml",
    "test": "jest",
    "test-integ": "jest -c jest-integ.config.js",
    "precompile": "rm -rf lib/**",
    "compile": "tsc",
    "build": "run-s generate lint compile",
    "lint": "eslint src/**/*.ts",
    "prepare": "run-s build test",
    "prepublishOnly": "run-s test-integ"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanryanmoore/scout-api-js.git"
  },
  "author": "Jordan Ryan Moore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordanryanmoore/scout-api-js/issues"
  },
  "homepage": "https://github.com/jordanryanmoore/scout-api-js#readme",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.8-4.2.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^25.1.0",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/pusher-js": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "acorn": "^7.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "jsonwebtoken": "^8.5.1",
    "pusher-js": "5.0.x"
  },
  "engines": {
    "node": ">=8.0"
  }
}
